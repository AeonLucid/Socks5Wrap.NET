<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>1.0.0</Version>
    <Authors>AeonLucid</Authors>
    <Company>AeonLucid</Company>
    <Description>A C# library to consume SOCKS5 proxies using netstandard2.0.</Description>
    <Copyright>AeonLucid</Copyright>
    <PackageProjectUrl>https://github.com/AeonLucid/Socks5Wrap.NET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/AeonLucid/Socks5Wrap.NET</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>SOCKS5</PackageTags>
    <PackageLicenseUrl>https://github.com/AeonLucid/Socks5Wrap.NET/blob/master/LICENSE</PackageLicenseUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
  </PropertyGroup>

</Project>
